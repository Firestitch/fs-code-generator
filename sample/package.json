{
  "name": "boilerplate",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "cd tools && npm-cross-var npm run build --env=$npm_package_config_env --platform=$npm_package_config_platform",
    "build:optimized": "cd tools && npm-cross-var npm run build:optimized --env=$npm_package_config_env --platform=$npm_package_config_platform",
    "package:web": "cd tools && npm-cross-var npm run package:web --env=$npm_package_config_env",
    "package:app": "cd tools && npm-cross-var npm run package:app --env=$npm_package_config_env",
    "package:web:prod": "npm run package:web --boilerplate:env=prod",
    "package:app:prod": "npm run package:app --boilerplate:env=prod",
    "serve": "cd tools && npm-cross-var npm run serve:run --env=$npm_package_config_env --platform=$npm_package_config_platform",
    "serve:livereload": "cd tools && npm-cross-var npm run serve:livereload --env=$npm_package_config_env --platform=$npm_package_config_platform",
    "serve:aot": "cd tools && npm-cross-var npm run serve:aot --env=$npm_package_config_env --platform=$npm_package_config_platform",
    "serve:secure": "cd tools && npm-cross-var npm run serve:secure --env=$npm_package_config_env --platform=$npm_package_config_platform",
    "serve:app": "cd tools && npm-cross-var npm run serve --env=$npm_package_config_env --platform=app",
    "serve:dev": "cd tools && npm-cross-var npm run serve --env=dev --platform=$npm_package_config_platform",
    "codegen": "node ./node_modules/@firestitch/codegenerator/dist/server.js"
  },
  "config": {
    "env": "local",
    "platform": "web"
  },
  "private": true,
